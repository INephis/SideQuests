public class Main {
    public static void main(String[] args) {
        int[] nums = {1, 2, 7, 8, 11, 15};
        int target = 9;


        for (int i = 0; i < nums.length-1; i++) {
            for (int j = i + 1; j < nums.length; j++) {
                if(nums[i] + nums[j] == target)
                    System.out.print("[" + i + ", "+ j + "]");
            }
            System.out.println();
        }
        

    }
}
